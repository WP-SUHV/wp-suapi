{
  "name": "wp-suapi",
  "description": "Wordpress plugin to connect with the swiss unihockey API v2.",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "keywords": [],
  "homepage": "https://github.com/WP-SUHV/wp-suapi",
  "authors": [
    {
      "name": "Philipp Meier",
      "email": "philipp.meier@fbriders.ch",
      "homepage": "https://github.com/meip",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/humanmade/Custom-Meta-Boxes"
    },
    {
      "type": "vcs",
      "url": "https://github.com/scribu/wp-posts-to-posts"
    }
  ],
  "extra" : {
    "installer-paths" : {
      "vendor/{$vendor}/{$name}/": ["type:wordpress-plugin"]
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~6.0",
    "kevinrob/guzzle-cache-middleware": "*",
    "doctrine/cache": "^1.0",
    "twig/twig": "^2.0",
    "jjgrainger/wp-custom-post-type-class": "dev-master",
    "humanmade/custom-meta-boxes": "dev-master",
    "scribu/lib-posts-to-posts": "dev-master",
    "scribu/posts-to-posts": "^1.6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "<6.0",
    "10up/wp_mock": "dev-dev"
  },
  "autoload": {
    "psr-4": {
      "WP_SUAPI\\": "includes"
    }
  }
}
