{
  "name": "wp-suapi",
  "description": "Wordpress plugin to connect with the swiss unihockey API v2.",
  "version": "0.1.0",
  "type": "wordpress-plugin",
  "keywords": [],
  "homepage": "https://github.com/WP-SUHV/wp-suapi",
  "authors": [
    {
      "name": "Philipp Meier",
      "email": "philipp.meier@fbriders.ch",
      "homepage": "https://github.com/meip",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gizburdt/cuztom"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4",
    "nategood/httpful": "*",
    "gizburdt/cuztom": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "10up/wp_mock": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "WP_SUAPI\\": "includes"
    },
    "classmap": [
      "vendor/gizburdt/cuztom/cuztom.php",
      "vendor/gizburdt/cuztom/classes"
    ]
  }
}
